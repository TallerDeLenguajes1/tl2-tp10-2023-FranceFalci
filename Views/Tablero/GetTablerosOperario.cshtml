              <path
                d="M14.1818 12C14.1818 12.7956 13.8924 13.5587 13.3773 14.1213C12.8622 14.6839 12.1635 15 11.435 15C10.7066 15 10.0079 14.6839 9.4928 14.1213C8.97768 13.5587 8.68829 12.7956 8.68829 12C8.68829 11.2044 8.97768 10.4413 9.4928 9.87868C10.0079 9.31607 10.7066 9 11.435 9C12.1635 9 12.8622 9.31607 13.3773 9.87868C13.8924 10.4413 14.1818 11.2044 14.1818 12Z"
                stroke="#326192" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
              <path
                d="M2.41559 12C3.58204 7.943 7.05302 5 11.1521 5C15.2521 5 18.7221 7.943 19.8886 12C18.7221 16.057 15.2521 19 11.1521 19C7.05302 19 3.58204 16.057 2.41559 12Z"
                stroke="#326192" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
            </svg>

          </a>
        </div>
      </div>
           
        </div>
    }
